<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class catalog="RBC1B" name="uk.org.rbc1b.roms.db.Attendance" table="Attendance">
        <composite-id name="attendanceid" class="uk.org.rbc1b.roms.db.AttendanceID">
            <key-property name="rbcid" type="long">
                <column name="RBCID" />
            </key-property>
            <key-property name="projectname" type="string">
                <column name="ProjectName" />
            </key-property>
            <key-property name="attenddate" type="date">
                <column name="AttendDate" />
            </key-property>
        </composite-id>
        <property name="department" type="string">
            <column name="Department" />
        </property>
    </class>
</hibernate-mapping>   