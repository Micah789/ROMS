<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping  package="uk.org.rbc1b.roms.db.project">
    <class name="ProjectStage" table="ProjectStage">
        <id name="projectStageId" >
            <generator class="identity"/>
        </id>
        <property name="name" length="5" />
        <property name="description" length="50" />
        <property name="assignedTo" length="500" />
        <property name="workNotes" length="1000" />
    </class>
</hibernate-mapping>



