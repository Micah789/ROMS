<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="uk.org.rbc1b.roms.db.volunteer">
  <class name="Volunteer" table="Volunteer">
    <id name="personId">
      <generator class="assigned"/>
    </id>
    <many-to-one class="uk.org.rbc1b.roms.db.Person" column="PersonId" lazy="false" name="person" unique="true"/>
    <many-to-one column="RbcStatusId" name="rbcStatus"/>
    <many-to-one column="AppointmentId" name="appointment"/>
    <many-to-one column="FulltimeId" name="fulltime"/>
    <property length="7" name="availability"/>
    <many-to-one column="EmergencyContactId" name="emergencyContact"/>
    <many-to-one column="EmergencyContactRelationshipId" name="emergencyContactRelationship"/>
    <property column="DOB" name="dob"/>
    <property length="1" name="gender"/>
    <many-to-one column="MaritalStatusId" name="maritalStatus"/>
    <property name="baptismDate"/>
    <property name="interviewDate"/>
    <many-to-one column="InterviewerA" name="interviewerA"/>
    <many-to-one column="InterviewerB" name="interviewerB"/>
    <property length="150" name="interviewComments"/>
    <property name="joinedDate"/>
    <property name="formDate"/>
    <many-to-one column="InterviewStatusId" name="interviewStatus"/>
    <property name="oversight"/>
    <property length="50" name="oversightComments"/>
    <property name="reliefUK"/>
    <property length="50" name="reliefUKComments"/>
    <property name="reliefAbroad"/>
    <property length="50" name="reliefAbroadComments"/>
    <property column="HHCFormCode" length="15" name="hhcFormCode"/>
    <property name="badgeIssueDate"/>
  </class>
</hibernate-mapping>
