<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="uk.org.rbc1b.roms.db.congregation">
    <class name="CongregationContact" table="CongregationContact">
        <id name="congregationContactId" >
            <generator class="identity"/>
        </id>
        <many-to-one name="congregation" column="CongregationId" />
        <many-to-one name="congregationRole" column="CongregationRoleId" />
        <many-to-one name="person" column="PersonId" />
    </class>
</hibernate-mapping>