<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="uk.org.rbc1b.roms.db.congregation">
    <class name="Circuit" table="Circuit">
        <id name="circuitId" >
            <generator class="identity"/>
        </id>
        <property name="name" length="50" unique="true" />
        <many-to-one name="circuitOverseer" class="uk.org.rbc1b.roms.db.Person" column="CircuitOverseerID" lazy="false" cascade="all"/>
    </class>
</hibernate-mapping>
